{
  "name": "cloudflare-mcp",
  "version": "0.1.0",
  "description": "MCP server for the entire Cloudflare API using codemode",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "npm run build:spec && wrangler deploy --env staging",
    "deploy:prod": "npm run build:spec && wrangler deploy --env production",
    "types": "wrangler types",
    "typecheck": "npm run types && tsc --noEmit",
    "build:spec": "tsx scripts/build-spec.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cloudflare/workers-oauth-provider": "^0.2.2",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "hono": "^4.7.10",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.11",
    "@types/node": "^25.0.6",
    "openapi-types": "^12.1.3",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "3.2",
    "wrangler": "^4.64.0"
  }
}
